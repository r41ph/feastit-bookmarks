{
  "extends": ["react-app", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 100,
        "bracketSpacing": true,
        "jsxBracketSameLine": false
      }
    ]
  },
  "plugins": ["prettier"]
}
